/* components/movable-custom-view/movable-custom-view.wxss */

.move-view {
  width: 100%;
  height: 100%;
}

.img-view {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100rpx;
}

.home-img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  overflow: hidden;
}

.home-txt {
  margin-top: 10rpx;
  font-size: 24rpx;
  background-color: rgba(230,230,230,0.7);
  color: #fff;
}
